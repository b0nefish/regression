		device	zxspectrum48
		org	#C000
		define fx 6

@_fxpage = #16
@_fxpage_start

;;-----------------------------------------------------------------------------
		include "music/pt3extra.a80"
		incbin "music/final.pt3",99

MUSIC.page = _fxpage
MUSIC.init = music_init
MUSIC.play = music_play

;;-----------------------------------------------------------------------------
fx_surprise:	incbin "surprise/final.pak"
.pg = _fxpage
.sync = 2540

;;-----------------------------------------------------------------------------
	if $ > #FFFF
		display "Too long! ",/D,($-#10000)," bytes out!"
		end
	else
		display /D,(#10000-$)," bytes free..."
	endif

		align	16384
		savebin "output",(_fxpage_start),$-(_fxpage_start)

;;-----------------------------------------------------------------------------

		export	MUSIC.page
		export	MUSIC.init
		export	MUSIC.play

		export fx_surprise
		export fx_surprise.pg
		export fx_surprise.sync
